{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://mdstudio/schemas/endpoints/deltatag_input.v1.json",
  "title": "Deltatag input",
  "description": "Deltatag",
  "type": "object",
  "properties": {
    "dataframe": {
      "$ref": "resource://mdgroup/common_resources/path_file/v1",
      "description": "Path to the dataframe"
    },
    "alpha_beta_gamma": {
      "type": "array",
      "default": [
        0.5,
        0.5,
        0.0
      ]
    },
    "kBt": {
      "type": "number",
      "default": 2.49
    },
    "fileformat": {
      "type": "string",
      "default": "csv"
    },
    "workdir": {
      "type": "string",
      "default": "."
    }
  },
  "required": [
    "dataframe"
  ]
}

